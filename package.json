{
  "name": "generator-wordpress-starter",
  "version": "0.1.1",
  "description": "A simple yeoman generator for Wordpress projects.",
  "homepage": "",
  "author": {
    "name": "codekraft-studio",
    "email": "info@codekraft.it",
    "url": "https://codekraft.it"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "wordpress",
    "starter",
    "wp-theme",
    "wp-plugin",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-xo-space": "^0.16.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "nsp": "^2.6.3",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.6.0"
  },
  "dependencies": {
    "yeoman-generator": "^1.1.1",
    "chalk": "^1.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint . --ignore-path .gitignore --fix",
    "test": "jest --coverage"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "no-unused-vars": 1,
      "no-undef": 1,
      "no-compare-neg-zero": 0,
      "template-tag-spacing": 0,
      "no-warning-comments": 0
    }
  },
  "repository": "codekraft-studio/generator-wordpress-starter",
  "license": "Apache-2.0"
}
